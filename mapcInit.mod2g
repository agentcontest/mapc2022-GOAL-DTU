use "knowledge_generated" as knowledge.

use "modules/percepts/getInitialPercepts".
use "modules/percepts/initBeliefState".
use "modules/goals/initGoals".
use "modules/communication/initCommunication".

module mapcInit {
	% Get initial percepts from environment
	if true then getInitialPercepts.
	
	% Set the initial belief state from given percepts
	if true then initBeliefState.
	
	% Adopt initial goals
	if true then initGoals.
	
	% Set initial communication channels
	if true then initCommunication.
}